{
	"name"	: "Tab List",
	"version"	: "1.0",
	"description" : "Shows list of opend tab details",
	"permissions" : [
		"tabs"
	],
	"browser_action" : {
		"default_title"	: "demo tab app",
		"default_popup" : "popup.html"
	},
	"content_scripts"	: [{
		"matches"	:	["http://*/*", "https://*/*"],
		"js"	:	["myscript.js"]
	}],
	"manifest_version"	: 2
}